{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs" ,
    "rootDir": "./src",
     "moduleResolution": "node",
     "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
     "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}